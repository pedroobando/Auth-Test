{
  "name": "apigqlcondominio",
  "version": "1.0.0",
  "description": "Api GraphQL para una aplicacion de condominio - usando una base de datos mongodb",
  "main": "src/index.js",
  "scripts": {
    "start": "node build/index.js",
    "dev": "nodemon .",
    "build": "node_modules/.bin/esbuild . --bundle --platform=node --minify --outfile=build/index.js"
  },
  "keywords": [
    "GraphQL",
